<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zyp.MOOC.vo.User" table="mooc_user">
        <id name="uid">
            <generator class="native"/>
        </id>
        <property name="pwd"/>
        <property name="uname"/>
        <property name="type"/>
    </class>


</hibernate-mapping>